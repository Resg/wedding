generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Rsvp {
  id        String   @id @default(cuid())
  fullName  String
  phone     String
  guests    Int
  attending Boolean  @default(true)
  createdAt DateTime @default(now())
}
